        <rdfs:label rdf:datatype="&xsd;string">player not in own half</rdfs:label>
        <skos:definition xml:lang="en">a player who is playing the match at the opponent&apos;s field. </skos:definition>
    </owl:Class>
    


    <!-- &so;Position -->
