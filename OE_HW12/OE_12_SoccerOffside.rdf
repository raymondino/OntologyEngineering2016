        <rdfs:label rdf:datatype="&xsd;string">soccer player</rdfs:label>
        <skos:definition xml:lang="en"> a sportsperson who plays one of the different types of football.</skos:definition>
        <skos:example xml:lang="en">The Player class describes a player who play soccer in the soccer match.</skos:example>
    </owl:Class>
    


    <!-- &so;PlayerAtOffsidePosition -->

    <owl:Class rdf:about="&so;PlayerAtOffsidePosition">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&so;Attacker"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&so;isNearerToDefenderGoalLineThan"/>
                        <owl:someValuesFrom rdf:resource="&so;InFieldBall"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&so;isNearerToDefenderGoalLineThan"/>
                        <owl:someValuesFrom rdf:resource="&so;SecondLastDefender"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&so;Attacker"/>
        <rdfs:label rdf:datatype="&xsd;string">player at offside position</rdfs:label>
        <skos:definition xml:lang="en">a person who is playing in a soccer match with his/her location is nearer than defender goal line than both the second last defender and ball.</skos:definition>
    </owl:Class>
    


    <!-- &so;PlayerCommitsOffsideOffence -->

    <owl:Class rdf:about="&so;PlayerCommitsOffsideOffence">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&so;PlayerAtOffsidePosition"/>
                    <rdf:Description rdf:about="&so;PlayerInvolvedInActivePlay"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dct:references rdf:datatype="&xsd;anyURI">http://www.fifa.com/mm/document/afdeveloping/refereeing/law_11_offside_en_47383.pdf</dct:references>
        <rdfs:label rdf:datatype="&xsd;string">offside offence player</rdfs:label>
        <skos:definition xml:lang="en">a player who commits the offside offence</skos:definition>
        <skos:example xml:lang="en">a player who commits an offside offence. Three  conditions must be met, 1. the player is an attacker, 2. The player is at offside position, 3, the player involes the active play.</skos:example>
    </owl:Class>
    


    <!-- &so;PlayerInOwnHalf -->

    <owl:Class rdf:about="&so;PlayerInOwnHalf">
        <rdfs:subClassOf rdf:resource="&so;Player"/>
        <owl:disjointWith rdf:resource="&so;PlayerNotInOwnHalf"/>
        <rdfs:label xml:lang="en">player in own half</rdfs:label>
        <skos:definition xml:lang="en">A player who is playing the match at his/her own side of the field.</skos:definition>
    </owl:Class>
    


    <!-- &so;PlayerInvolvedInActivePlay -->

