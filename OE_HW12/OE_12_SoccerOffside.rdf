            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&so;Position"/>
        <rdfs:label rdf:datatype="&xsd;string">describes the player&apos;s position that is at this player&apos;s own half of the whole field.</rdfs:label>
        <skos:definition xml:lang="en">the spatial location of the players who are in their own half. </skos:definition>
    </owl:Class>
    


    <!-- &so;Player -->

    <owl:Class rdf:about="&so;Player">
        <rdfs:subClassOf rdf:resource="&fibo-fnd-aap-ppl;Person"/>
        <owl:equivalentClass rdf:resource="&dbo;SoccerPlayer"/>           
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
