                    <rdf:Description rdf:about="&so;player_A6"/>
                    <rdf:Description rdf:about="&so;player_A7"/>
                    <rdf:Description rdf:about="&so;player_B1"/>
                    <rdf:Description rdf:about="&so;player_B2"/>
                    <rdf:Description rdf:about="&so;player_B3"/>
                    <rdf:Description rdf:about="&so;player_B4"/>
                    <rdf:Description rdf:about="&so;player_B5"/>
                    <rdf:Description rdf:about="&so;player_B6"/>
                    <rdf:Description rdf:about="&so;player_B7"/>
                </owl:oneOf>
            </owl:Class>
