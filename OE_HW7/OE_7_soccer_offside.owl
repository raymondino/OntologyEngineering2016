<?xml version="1.0"?>
<rdf:RDF xmlns="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/"
     xml:base="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/">
        <owl:versionIRI rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/1.0.1"/>
        <rdfs:comment xml:lang="en">This ontology was created on 3/18/2016. 
This ontology was updated on 3/25/2016 for version 1.0.1</rdfs:comment>
        <rdfs:comment xml:lang="en">Author: Rui Yan https://tw.rpi.edu//web/person/RuiYan 
Hao Zhong https://tw.rpi.edu//web/person/HaoZhong</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/contains -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/contains">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/frappe#Frame"/>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Record"/>
        <rdfs:comment xml:lang="en">a frame contains multiple records.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">contains</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/hasFrameInfo -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/hasFrameInfo">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/frappe#Frame"/>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/ActivePlay"/>
        <rdfs:comment xml:lang="en">this property connects frame and its information. An example is :Frame1 :hasFrameInfo :BallTouch. This facilitates the identification of semantic important frame (which defines as a frame that hasFrameInfo some active play). Not all frames contains some active play. Only those that contain some active play are considered as important.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has frame information</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/hasSensor -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/hasSensor">
        <rdfs:domain rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Record"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has sensor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/hasTeam -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/hasTeam">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/SoccerPlayer"/>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Team"/>
        <rdfs:comment xml:lang="en">hasTeam property is functional, because every Player individual can have at most 1 team.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has team</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/hasTeammate -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/hasTeammate">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/SoccerPlayer"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/SoccerPlayer"/>
        <rdfs:comment xml:lang="en">hasTeammate property is symmetric because A hasTeammate B is equivalent to B hasTeammate A.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has teammate</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/involves -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/involves">
        <owl:inverseOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/isInvolvedIn"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/SoccerPlayer"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Ball"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/ActivePlay"/>
        <rdfs:comment xml:lang="en">an active play involves a player</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">involves</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/isAt -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/isAt">
        <owl:inverseOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/isPositionOf"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/SoccerPlayer"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/position"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is at</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/isFartherToOwnGoalLineThan -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/isFartherToOwnGoalLineThan">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/SoccerPlayer"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/position"/>
        <rdfs:comment xml:lang="en">This property describes that a player is farther to own goal line than one of his/her teammates. The comparison will be done in the data pre-processing. This propety seems to be transitive, since A isFartherThan B, B isFartherThan C, then A isFartherThan C. However, reasoning error will occur if it is transitive. We couldn&apos;t figure out why (at least for now).</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is farther to own goal line than</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/isInvolvedIn -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/isInvolvedIn">
        <rdfs:domain rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/ActivePlay"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/SoccerPlayer"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Ball"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is involved in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/isNearerToDefenderGoalLineThan -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/isNearerToDefenderGoalLineThan">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/SoccerPlayer"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/SoccerPlayer"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Ball"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is nearer to defender goal line than</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/isPositionOf -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/isPositionOf">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/position"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/SoccerPlayer"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is position of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/touched -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/touched">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/SoccerPlayer"/>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Ball"/>
        <rdfs:comment xml:lang="en">the past tence &quot;touched&quot; indicates that the player is the last player who touched the ball</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">touched</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dbpedia.org/ontology/SoccerPlayer -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/SoccerPlayer">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/GoalkeeperA"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/GoalkeeperB"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerA1"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerA2"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerA3"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerA4"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerA5"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerA6"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerA7"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerB1"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerB2"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerB3"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerB4"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerB5"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerB6"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/PlayerB7"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/hasTeam"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Team"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">the soccer player in a soccer match. We use dbpedia defined concept SoccerPlayer. http://dbpedia.org/ontology/SoccerPlayer</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">player</rdfs:label>
    </owl:Class>
    


    <!-- http://dbpedia.org/ontology/position -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/position">
        <owl:disjointWith rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Observation"/>
        <owl:disjointWith rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
        <owl:disjointWith rdf:resource="http://streamreasoning.org/ontologies/frappe#Frame"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Ball"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Record"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Referee"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Team"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/RuiYan/SemanticImportance"/>
        <rdfs:comment xml:lang="en">We extend dbpedia ontology which defines position: http://dbpedia.org/ontology/position</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">position</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#Observation -->

    <owl:Class rdf:about="http://purl.oclc.org/NET/ssnx/ssn#Observation">
        <owl:disjointWith rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
        <owl:disjointWith rdf:resource="http://streamreasoning.org/ontologies/frappe#Frame"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/ActivePlay"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Ball"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Record"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Referee"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Team"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/RuiYan/SemanticImportance"/>
        <rdfs:comment xml:lang="en">We reuse SSN ontology observation class. http://purl.oclc.org/NET/ssnx/ssn#Observation</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sensor observation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#Sensor -->

    <owl:Class rdf:about="http://purl.oclc.org/NET/ssnx/ssn#Sensor">
        <owl:disjointWith rdf:resource="http://streamreasoning.org/ontologies/frappe#Frame"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/ActivePlay"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Ball"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Record"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Referee"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Team"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/RuiYan/SemanticImportance"/>
        <rdfs:comment xml:lang="en">We use SSN ontology, which defines what a sensor is. https://www.w3.org/2005/Incubator/ssn/ssnx/ssn#term-Sensor</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sensor</rdfs:label>
    </owl:Class>
    


    <!-- http://streamreasoning.org/ontologies/frappe#Frame -->

    <owl:Class rdf:about="http://streamreasoning.org/ontologies/frappe#Frame">
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/ActivePlay"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Ball"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Record"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Referee"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Team"/>
        <rdfs:comment xml:lang="en">We re-use the Frame concept in FraPPE ontology. http://streamreasoning.org/ontologies/frappe#Frame
Balduini M, Della Valle E. FraPPE: a vocabulary to represent heterogeneous spatio-temporal data to support visual analytics[M]//The Semantic Web-ISWC 2015. Springer International Publishing, 2015: 321-328.

We use the frame class to describe all positions on the soccer field within 0.0005 second. The frame class will be used when processing the data, and answering the query.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">frame</rdfs:label>
    </owl:Class>
    


    <!-- http://streamreasoning.org/ontologies/frappe#samplingTime -->

    <owl:Class rdf:about="http://streamreasoning.org/ontologies/frappe#samplingTime">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Observation"/>
        <rdfs:comment xml:lang="en">We reuse FraPPE ontology, which defines sampling time. http://streamreasoning.org/ontologies/frappe#samplingTime
Balduini M, Della Valle E. FraPPE: a vocabulary to represent heterogeneous spatio-temporal data to support visual analytics[M]//The Semantic Web-ISWC 2015. Springer International Publishing, 2015: 321-328.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sampling timestamp</rdfs:label>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/ActivePlay -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/ActivePlay">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/GainingAdvantage"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/InterferingOpponent"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/InterferingPlay"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Record"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Referee"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Team"/>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/RuiYan/SemanticImportance"/>
        <rdfs:comment xml:lang="en">Active play is either interfering with play, interfering with an opponent or gaining an advantage by being in that position.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">active play</rdfs:label>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Attacker -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Attacker">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/SoccerPlayer"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/BallLastToucher"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/hasTeammate"/>
                                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                                <owl:onClass rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/BallLastToucher"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_6_Soccer_Offside/Defender"/>
        <rdfs:comment xml:lang="en">An attacker is a player who aims to kick the ball into the opponent&apos;s goal. An attacker is not a defender.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">attacker</rdfs:label>
    </owl:Class>
    


