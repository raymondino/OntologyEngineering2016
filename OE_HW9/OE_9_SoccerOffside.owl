     xmlns:so="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/">


    <owl:Ontology rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/">
        <dct:publisher>Soccer Offside Team</dct:publisher>
        <dct:contributor>Rui Yan</dct:contributor>
        <dct:contributor>Hao Zhong</dct:contributor>
        <dct:abstract>The use case aims to assist referees to determine if an offside offense is committed by the attacking players, and increase the judgement precision.
        </dct:abstract>
        <dct:modified>2016-04-08</dct:modified>
        <owl:versionIRI rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/2.1.0"/>
        <skos:changeNote>In this version, we have fixed the inference issue of "team is player". We also add a property chain to enable attacker/defender inference.
        </skos:changeNote>
        <owl:priorVersion rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/2.0.0"/>
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    </owl:Ontology>

