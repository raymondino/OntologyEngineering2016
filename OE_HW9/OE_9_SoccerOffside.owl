     -->

    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/ActivePlay -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/ActivePlay">
        <dct:bibliographicCitation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FIFA. 2015/2016. Laws of the game. Available:http://www.fifa.com/mm/Document/FootballDevelopment/Refereeing/02/36/01/11/LawsofthegamewebEN_Neutral.pdf</dct:bibliographicCitation>
        <rdfs:comment xml:lang="en">We decided to only mention this class in the ontology. We will directly annotate a player who interferes play or opponent with &quot;involves ActivePlay&quot;.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">active play</rdfs:label>
        <skos:definition xml:lang="en">no direct definition while subclasses are defined in LOTG such as interefing with play, interfering with opponents, and gaining advantage</skos:definition>
        <skos:example xml:lang="en">ActivePlay class is used to describe the active play involved by the players. This class contains two individuals, BallTouch and OpponentChallenge, which are two normal cases of the active play. These two instances will be used to help infer which player commits the offside offence.</skos:example>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Attacker -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Attacker">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/BallLastToucher"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/hasTeammate"/>
                        <owl:someValuesFrom rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/BallLastToucher"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
        <dct:bibliographicCitation xml:lang="en">Google. Available: https://www.google.com/webhp?sourceid=chrome-instant&amp;ion=1&amp;espv=2&amp;es_th=1&amp;ie=UTF-8#q=attacker+definition</dct:bibliographicCitation>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">attacker</rdfs:label>
        <skos:definition xml:lang="en">a player that makes an assertive or aggressive attempt to score</skos:definition>
        <skos:example xml:lang="en">Attacker class describes the players who are attacking. Attacker is composed by a ball toucher and his/her teammates.</skos:example>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Ball -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Ball">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/SoccerBall"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <dct:references rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Ball_(association_football)</dct:references>
        <rdfs:comment xml:lang="en">the soccer ball class, defined by one instance &quot;SoccerBall&quot;.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the soccer ball</rdfs:label>
        <skos:definition xml:lang="en">A football, soccer ball, or association football ball is the ball used in the sport of association football.</skos:definition>
        <skos:example xml:lang="en">Ball class is defined by one instance SoccerBall. It describes a ball used in a soccer match.</skos:example>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/BallLastToucher -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/BallLastToucher">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/touch"/>
                        <owl:someValuesFrom rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Ball"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/involves"/>
                <owl:someValuesFrom rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/ActivePlay"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dct:bibliographicCitation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FIFA. 2015/2016. Laws of the game. Available:http://www.fifa.com/mm/Document/FootballDevelopment/Refereeing/02/36/01/11/LawsofthegamewebEN_Neutral.pdf</dct:bibliographicCitation>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the ball toucher</rdfs:label>
        <skos:definition xml:lang="en">The latest player who touches the ball at the moment</skos:definition>
        <skos:example xml:lang="en">BallLastToucher describes the last player who touches the ball at the latest moment. A player whoever touches the ball is an attacker, as his/her team controls the ball.</skos:example>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/BallPosition -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/BallPosition">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Position"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/isPositionOf"/>
                        <owl:someValuesFrom rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Ball"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Position"/>
        <dct:bibliographicCitation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FIFA. 2015/2016. Laws of the game. Available:http://www.fifa.com/mm/Document/FootballDevelopment/Refereeing/02/36/01/11/LawsofthegamewebEN_Neutral.pdf</dct:bibliographicCitation>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ball position</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the position of the ball</skos:definition>
        <skos:example xml:lang="en">BallPosition describes the soccer ball&apos;s position.</skos:example>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Defender -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Defender">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/playsAgainst"/>
                        <owl:someValuesFrom rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Attacker"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
        <dct:bibliographicCitation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Google. Available: https://www.google.com/webhp?sourceid=chrome-instant&amp;ion=1&amp;espv=2&amp;es_th=1&amp;ie=UTF-8#q=defender%20definition</dct:bibliographicCitation>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the defending player</rdfs:label>
        <skos:definition xml:lang="en">a player whose task it is to protect the team&apos;s goal.</skos:definition>
        <skos:example xml:lang="en">Defender class describes the players who tries to defend their goal. They don&apos;t control the ball.</skos:example>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/OffsidePlayer -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/OffsidePlayer">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Attacker"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/hasPosition"/>
                        <owl:someValuesFrom rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/OffsidePosition"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/involves"/>
                        <owl:someValuesFrom rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/ActivePlay"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Attacker"/>
        <dct:references rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.fifa.com/mm/document/afdeveloping/refereeing/law_11_offside_en_47383.pdf</dct:references>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">offside offence player</rdfs:label>
        <skos:definition xml:lang="en">a player who commits the offside offence</skos:definition>
        <skos:example xml:lang="en">An offside player is a player who commits an offside offence. Three  conditions must be met, 1. the player is an attacker, 2. The player is at offside position, 3, the player involes the active play.</skos:example>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/OffsidePosition -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/OffsidePosition">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
