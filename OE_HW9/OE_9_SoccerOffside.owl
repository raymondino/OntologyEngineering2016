    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/hasPosition -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/hasPosition">
        <owl:inverseOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/isPositionOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Ball"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Position"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a player or ball has a position</dct:source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has position</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/hasTeam -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/hasTeam">
        <owl:inverseOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/isTeamOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Team"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a player has team</dct:source>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/hasTeammate -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/hasTeammate">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a player has a teammate</dct:source>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/involves -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/involves">
        <rdfs:domain rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/ActivePlay"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a player involves an active play</dct:source>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/isNearerToDefenderGoalLineThan -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/isNearerToDefenderGoalLineThan">
        <rdfs:domain rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Position"/>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Position"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a player position is nearer to defender&apos;s goal line than a ball or another player&apos;s position</dct:source>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/isOpponentOf -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/isOpponentOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">different players in different teams are opponents.</dct:source>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/isPositionOf -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/isPositionOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Position"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Ball"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a position of a player/ball</dct:source>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/isTeamOf -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/isTeamOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Team"/>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a team of a player</dct:source>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/playsAgainst -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/playsAgainst">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/hasTeammate"/>
            <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/isOpponentOf"/>
        </owl:propertyChainAxiom>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">different palyers in different teams play against each other</dct:source>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/touch -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/touch">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Player"/>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/Soccer_Offside/Ball"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a player touches the ball</dct:source>
    </owl:ObjectProperty>
    


