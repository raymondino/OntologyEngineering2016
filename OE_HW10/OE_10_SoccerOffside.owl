    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_10_Soccer_Offside/ActivePlay -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_10_Soccer_Offside/ActivePlay">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_10_Soccer_Offside/BallTouch"/>
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_10_Soccer_Offside/OpponentChallenge"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <dct:bibliographicCitation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FIFA. 2015/2016. Laws of the game. Available:http://www.fifa.com/mm/Document/FootballDevelopment/Refereeing/02/36/01/11/LawsofthegamewebEN_Neutral.pdf</dct:bibliographicCitation>
        <rdfs:comment xml:lang="en">We decided to only mention this class in the ontology. We will directly annotate a player who interferes play or opponent with &quot;involves ActivePlay&quot;.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">active play</rdfs:label>
        <skos:definition xml:lang="en">no direct definition while subclasses are defined in LOTG such as interefing with play, interfering with opponents, and gaining advantage</skos:definition>
        <skos:example xml:lang="en">ActivePlay class is used to describe the active play involved by the players. This class contains two individuals, BallTouch and OpponentChallenge, which are two normal cases of the active play. These two instances will be used to help infer which player commits the offside offence.</skos:example>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_10_Soccer_Offside/Attacker -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_10_Soccer_Offside/Attacker">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_10_Soccer_Offside/BallLastToucher"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_10_Soccer_Offside/hasTeammate"/>
                        <owl:someValuesFrom rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_10_Soccer_Offside/BallLastToucher"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_10_Soccer_Offside/Player"/>
        <dct:bibliographicCitation xml:lang="en">Google. Available: https://www.google.com/webhp?sourceid=chrome-instant&amp;ion=1&amp;espv=2&amp;es_th=1&amp;ie=UTF-8#q=attacker+definition</dct:bibliographicCitation>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">attacker</rdfs:label>
        <skos:definition xml:lang="en">a player that makes an assertive or aggressive attempt to score</skos:definition>
        <skos:example xml:lang="en">Attacker class describes the players who are attacking. Attacker is composed by a ball toucher and his/her teammates.</skos:example>
    </owl:Class>
    


